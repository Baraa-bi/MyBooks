{
  "expo": {
    "name": "MyBooks",
    "slug": "MyBooks",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.baraa.mybooks.ios"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.baraa.mybooks.android"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["./platform-colors.plugin.js"],
    "platformColors": {
      "text": {
        "light": "#1f2937",
        "dark": "#ffffff"
      },
      "primary": {
        "light": "#e11d48",
        "dark": "#e11d48"
      },
      "background": {
        "light": "#ffffff",
        "dark": "#1f2937"
      },
      "foreground": {
        "light": "#f3f4f6",
        "dark": "#334155"
      }
    }
  }
}
